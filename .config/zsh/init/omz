export ZSH="${XDG_DATA_HOME}/ohmyzsh"

CASE_SENSITIVE="true"
DISABLE_AUTO_UPDATE="true"
HIST_STAMPS="yyyy-mm-dd"
ZSH_CUSTOM="${XDG_DATA_HOME}/omz-custom"
ZSH_COMPDUMP="${XDG_CACHE_HOME}/zsh/zcompdump-${HOST/.*/}-${ZSH_VERSION}"

plugins=(
  dotenv
  git
  gpg-agent
  ssh-agent
  vi-mode
  virtualenvwrapper
  zsh-autosuggestions
)
